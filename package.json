{
  "name": "rarecanvas-web3-app",
  "version": "1.27.0",
  "private": true,
  "description": "Rare Canvas Web3 DApp built on Klever Blockchain",
  "scripts": {
    "dev": "pkill -f next 2>/dev/null; next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "validate:build": "npm run build",
    "precommit": "npm run validate",
    "pregit": "npm run validate:build",
    "dev:clean": "rm -rf .next node_modules/.cache .turbo && pkill -f next 2>/dev/null; next dev",
    "dev:strict": "npm run type-check && next dev",
    "swap-bot": "npx ts-node scripts/telegram-swap-bot.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@klever/sdk": "^1.0.0",
    "@klever/sdk-node": "^2.4.2",
    "@klever/sdk-web": "^1.5.1",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@upstash/redis": "^1.35.7",
    "@vercel/analytics": "^1.6.1",
    "bip39": "^3.1.0",
    "ed25519-hd-key": "^1.3.0",
    "eslint-config-next": "^15.5.7",
    "geist": "^1.5.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.556.0",
    "next": "^15.5.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.22",
    "baseline-browser-mapping": "^2.9.15",
    "eslint": "^9.39.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  }
}
